	//이전 슬라이드  7 6 5 4 3 2 1 0  포지션이 좌측에서 우측으로 이동 
	function prevSlide7(){
		sFormat();
		navBtnFn(7);
		s[7]=1; //여덟번째 슬라이드 실행중임			
		$('.slide').eq(0).stop().animate({left:(100* 0)+'%'},0).animate({left:(100* 1)+'%'},1000,'easeOutBack');
		$('.slide').eq(7).stop().animate({left:(100*-1)+'%'},0).animate({left:(100* 0)+'%'},1000,'easeOutBack');
		$('.slide').eq(6).stop().animate({left:(100*-2)+'%'},0).animate({left:(100*-1)+'%'},1000,'easeOutBack');
		$('.slide').eq(5).stop().animate({left:(100*-3)+'%'},0).animate({left:(100*-2)+'%'},1000,'easeOutBack');
		$('.slide').eq(4).stop().animate({left:(100*-4)+'%'},0).animate({left:(100*-3)+'%'},1000,'easeOutBack');
		$('.slide').eq(3).stop().animate({left:(100*-5)+'%'},0).animate({left:(100*-4)+'%'},1000,'easeOutBack');
		$('.slide').eq(2).stop().animate({left:(100*-6)+'%'},0).animate({left:(100*-5)+'%'},1000,'easeOutBack');
		$('.slide').eq(1).stop().animate({left:(100*-7)+'%'},0).animate({left:(100*-6)+'%'},1000,'easeOutBack');
	}
