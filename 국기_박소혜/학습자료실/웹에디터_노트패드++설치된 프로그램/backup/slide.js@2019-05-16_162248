		$('.slide').eq(6).stop().animate({left:(100*-4)+'%'},0).animate({left:(100*-3)+'%'},1000,'easeOutBack');
		$('.slide').eq(5).stop().animate({left:(100*-5)+'%'},0).animate({left:(100*-4)+'%'},1000,'easeOutBack');
		$('.slide').eq(4).stop().animate({left:(100*-6)+'%'},0).animate({left:(100*-5)+'%'},1000,'easeOutBack');
		$('.slide').eq(3).stop().animate({left:(100*-7)+'%'},0).animate({left:(100*-6)+'%'},1000,'easeOutBack');
	}
	function prevSlide0(){
		navBtnFn(0);
